{
  "name": "@unibeautify/beautifier-prettier",
  "version": "0.3.1",
  "description": "Prettier beautifier for Unibeautify",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "ava": "ava --verbose",
    "test": "npm run ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unibeautify/beautifier-prettier.git"
  },
  "keywords": [
    "prettier",
    "unibeautify",
    "beautifier"
  ],
  "author": "Glavin Wiechert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unibeautify/beautifier-prettier/issues"
  },
  "homepage": "https://github.com/Unibeautify/beautifier-prettier#readme",
  "dependencies": {
    "prettier": "^1.9.2",
    "unibeautify-beautifier": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^8.5.8",
    "@types/prettier": "^1.9.0",
    "ava": "^0.24.0",
    "typescript": "^2.6.2",
    "unibeautify": "^0.3.0"
  },
  "providedServices": {
    "unibeautify": {
      "versions": {
        "1.0.0": "provideBeautifier"
      }
    }
  },
  "package-deps": [
    "atom-beautify"
  ]
